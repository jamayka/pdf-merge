{
    "name": "xiag/pdf-merge",
    "description": "PDF merge HTTP tool",

    "require": {
        "php": ">=5.3.3",
        "symfony/http-foundation": "2.3.*@dev",
        "zendframework/zend-stdlib": "dev-master",
        "zendframework/zendpdf": "2.0.*",
        "guzzle/http": "3.1.*"
    },

    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "mockery/mockery": "dev-master"
    },

    "autoload": {
        "psr-0": {
            "PdfMerge": "src/"
        },

        "files": [
            "src/PdfMerge/common.php",
            "src/PdfMerge/Concatenate/functions.php"
        ]
    }
}